# .env.development
# URL du backend - utiliser le backend déployé même en dev
VITE_API_URL=https://api-dev.dev-maturity.e-dsin.fr

# Configuration de l'environnement
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0-dev

# Configuration du debug (activé en développement)
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Configuration API
VITE_API_TIMEOUT=30000

# Frontend URL (pour référence)
VITE_FRONTEND_URL=http://localhost:5173

# Désactiver l'authentification bypass par défaut
# Décommentez la ligne suivante pour activer le bypass en dev local
# VITE_BYPASS_AUTH=true