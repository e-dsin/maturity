    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.15.3",
    "redis": "^5.1.0",
    "swagger-tools": "^0.10.4",
    "swagger-ui-express": "^5.0.1",
