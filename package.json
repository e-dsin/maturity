    "axios": "^1.6.7",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^8.2.2",
    "connect-redis": "^8.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
