{
  "name": "maturity-assessment-app",
  "version": "1.0.0",
  "description": "Application d'évaluation de maturité avec API Axios",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run start:server\"",
    "dev": "vite --host 0.0.0.0",
    "start:server": "node server/server.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "setup-db": "ts-node scripts/setup-database.ts",
    "update-db": "ts-node scripts/update-database.ts --drop --skip-backup",
    "migrate-data": "ts-node scripts/migrate-data.ts",
    "reset-seed:db": "ts-node scripts/resetAndSeedDatabase.ts",
    "seedGrille:db": "ts-node scripts/seedGrilleInterpretation.ts",
    "setup-database": "ts-node scripts/setupDatabaseWithMaturity.ts",
    "import-devsecops": "ts-node scripts/import-devsecsops-questionnaire.ts",
    "test-auth": "node scripts/test-auth-endpoint.js",
    "migrate-score": "node scripts/create-enterprise-history.js",
    "test": "vitest",
    "test:api": "node tests/test-api.js",
    "generate-data": "ts-node-esm ./mocks/dataGenerator.ts",
    "generate-swagger": "node scripts/generate-swagger-docs.js",
    "generate:postman": "node scripts/export-to-postman.js",
    "generate-all-docs": "node scripts/generate-all-docs.js",
    "create-admin-user": "node scripts/create-admin-user.js"
